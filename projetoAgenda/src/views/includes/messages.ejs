<% if(errors.length > 0) {%>

    <div class="error-message">
        <% errors.forEach(erro=>{ %>
            <p class="error-text"><%= erro %></p>
        <% }); %> 
    </div>

<% } %> 

<% if(success.length > 0) {%>

    <div class="success-message">
        <% success.forEach(succes=>{ %>
            <p class="success-text"><%= succes %></p>
        <% }); %> 
    </div>

<% } %> 